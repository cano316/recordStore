<%- include('partials/header.ejs') %>
    <h1>All Products</h1>
    <a href="/products/new">Add New Product</a>
    <% for(let product of products) { %>
        <h3>
            <%= product.title %>
        </h3>
        <h4>
            <%= product.artist %>
        </h4>
        <h5>
            Price: $<%= product.price %>
        </h5>
        <a href="/products/<%= product.id %>">Details</a>
        <% } %>
            <%- include('partials/footer.ejs') %>